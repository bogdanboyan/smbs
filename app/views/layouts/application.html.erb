      <%= csrf_meta_tag %>
       <%= yield :head %>
    </head>
    <body>
       <div class="container_12">
         <div class="clear">&nbsp;</div>
 
         <div class="grid_12 head">
          <%=render :partial => "shared/header"%>
         </div>
 
         <div class="grid_12 page">
          <%= yield %>
        </div>
       </div>
    </body>
</html>
